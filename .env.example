# Configuração do LiveKit
LIVEKIT_API_KEY=your_api_key_here
LIVEKIT_API_SECRET=your_secure_api_secret_here

# Configuração do Redis
REDIS_PASSWORD=your_secure_redis_password

# Configuração da VPS
PUBLIC_IP=seu_ip_publico_aqui
DOMAIN=seu.dominio.com

# Segurança da API
API_AUTH_TOKEN=seu_token_seguro_aqui

# Configurações do LiveKit (opcional)
LIVEKIT_REGION=us-east-1
LIVEKIT_TURNS_ENABLED=true

# Configurações de Log
LOG_LEVEL=info
